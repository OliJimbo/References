	Journal = {Body Image},
	Keywords = {Media campaign, Physical functionality satisfaction, Appearance satisfaction, Social comparison, Exercise, Body functionality},
	Pages = {26 - 35},
	Title = {This girl can #jointhemovement: Effectiveness of physical functionality-focused campaigns for women's body satisfaction and exercise intent},
	Url = {http://www.sciencedirect.com/science/article/pii/S1740144517300499},
	Volume = {24},
	Year = {2018},
