	Author = {Darville, Gabrielle and Anderson --Lewis, Charkarra and Stellefson, Michael and Lee, Yu-Hao and MacInnes, Jann and Pigg, R. Morgan and Gilbert, Juan E. and Thomas, Sanethia},
	Booktitle = {Simulation \& Gaming},
	Da = {2018/10/01},
