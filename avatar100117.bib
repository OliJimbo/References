	Title = {Reducing HIV risk behavior of MSM through persuasive computing: results of the men's INTernet study (MINTS-II)},
	Ty = {JOUR},
	Url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC3319361/},
	Year = {2010},
