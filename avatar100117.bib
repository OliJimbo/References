	Doi = {http://doi.org/10.1016/j.jmp.2015.08.004},
	Isbn = {0022-2496},
	Journal = {Journal of Mathematical Psychology},
	Keywords = {Monotonicity; State-trace; Convex hulls; Order-constrained inference},
	Month = {6},
	Pages = {116--129},
	Title = {Bayes factors for state-trace analysis},
	Ty = {JOUR},
	Url = {http://www.sciencedirect.com/science/article/pii/S0022249615000528},
	Volume = {72},
	Year = {2016},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0022249615000528},
