	Journal = {Australian Journal of Psychology},
	Keywords = {barriers, health behaviour, healthy eating, knowledge, physical activity},
	Number = {4},
	Pages = {251-260},
	Title = {Beliefs as barriers to healthy eating and physical activity},
	Url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/ajpy.12103},
	Volume = {68},
	Year = {2016},
