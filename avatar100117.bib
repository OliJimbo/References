	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S1740144515000893},
	Bdsk-Url-2 = {https://doi.org/10.1016/j.bodyim.2015.06.003}}

@article{ROBINSON201765,
	Abstract = {Recent studies have documented a shift in the cultural ideal of physical attractiveness, with women subscribing to a visibly toned ideal that emphasises health and fitness. The present study experimentally investigated the impact of athletic and muscular fitness-idealised images compared to traditional thin ideal images on women's body dissatisfaction and exercise behaviour, under the framework of Social Comparison Theory. Participants were 106 female undergraduate students randomly assigned to view one of three sets of images (thin ideal, athletic ideal, or muscular ideal) followed by a bout of exercise. Acute exposure to athletic ideal and thin ideal images led to increased body dissatisfaction, but exposure to muscular ideal images did not. Relative to thin ideal images, fitness-idealised images did not motivate participants to engage in higher levels of exercise suggesting that this type of fitness inspiration might not motivate actual exercise behaviour.},
	Author = {Lily Robinson and Ivanka Prichard and Alyssa Nikolaidis and Claire Drummond and Murray Drummond and Marika Tiggemann},
	Date-Added = {2018-08-20 20:58:05 +0000},
	Date-Modified = {2018-08-20 20:58:05 +0000},
	Doi = {https://doi.org/10.1016/j.bodyim.2017.06.001},
	Issn = {1740-1445},
	Journal = {Body Image},
	Keywords = {Media, Exercise, Social comparison, Fitspiration, Muscularity},
	Pages = {65 - 71},
	Title = {Idealised media images: The effect of fitspiration imagery on body satisfaction and exercise behaviour},
	Url = {http://www.sciencedirect.com/science/article/pii/S1740144517300657},
	Volume = {22},
	Year = {2017},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S1740144517300657},
	Bdsk-Url-2 = {https://doi.org/10.1016/j.bodyim.2017.06.001}}

@article{HOMAN201250,
	Abstract = {Modern ideals of female attractiveness include an extremely toned and fit appearance in addition to extreme thinness. Although viewing thin models has a negative effect on women's body image, research has not tested the effect of exposure to the ultra-fit physique separate from the thin-ideal. This randomized, posttest-only experiment tested the effects of the athletic aspect of the current ideal by exposing 138 undergraduate women to thin and athletic models, normal weight athletic models, or a control condition consisting of neutral objects. The study also tested the moderating effects of thin-ideal and athletic-ideal internalization. Exposure to thin ultra-fit models, but not normal weight ultra-fit models, produced an increase in body dissatisfaction and neither internalization variable moderated this effect. Findings suggest that interventions that focus on the benefits of fitness while challenging the desirability of thinness may offer promising results.},
	Author = {Kristin Homan and Erin McHugh and Daniel Wells and Corrinne Watson and Carolyn King},
	Date-Added = {2018-08-20 20:58:01 +0000},
	Date-Modified = {2018-08-20 20:58:01 +0000},
	Doi = {https://doi.org/10.1016/j.bodyim.2011.07.006},
	Issn = {1740-1445},
	Journal = {Body Image},
	Keywords = {Media exposure, Athletic ideal, Internalization, College women, Body dissatisfaction, Experiment},
	Number = {1},
	Pages = {50 - 56},
	Title = {The effect of viewing ultra-fit images on college women's body dissatisfaction},
	Url = {http://www.sciencedirect.com/science/article/pii/S1740144511000982},
	Volume = {9},
	Year = {2012},
