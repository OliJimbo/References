	Doi = {http://dx.doi.org/10.1016/j.ijmedinf.2016.02.006},
	Isbn = {1386-5056},
	Journal = {International Journal of Medical Informatics},
	Keywords = {Gamification; Health; Behavior change support systems; Persuasive technology; Serious games; e-health},
	Month = {12},
	Pages = {62--70},
	Title = {Understanding persuasion contexts in health gamification: A systematic analysis of gamified health behavior change support systems literature},
	Ty = {JOUR},
