	Isbn = {0022-2496},
	Journal = {Journal of Mathematical Psychology},
	Keywords = {Odds; Bayesian; Frequentist; Bayes factors},
	Month = {6},
	Pages = {90--103},
	Title = {Rejection odds and rejection ratios: A proposal for statistical practice in testing hypotheses},
	Ty = {JOUR},
	Url = {http://www.sciencedirect.com/science/article/pii/S002224961600002X},
	Volume = {72},
	Year = {2016},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S002224961600002X},
