	Isbn = {0022-2496},
	Journal = {Journal of Mathematical Psychology},
	Keywords = {Default Bayes factor; Effect size; Error probabilities; One sample t test},
	Month = {6},
	Pages = {130--143},
	Title = {Error probabilities in default Bayesian hypothesis testing},
	Ty = {JOUR},
	Url = {http://www.sciencedirect.com/science/article/pii/S0022249615000565},
