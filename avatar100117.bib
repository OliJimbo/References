	Isbn = {0022-2496},
	Journal = {Journal of Mathematical Psychology},
	Keywords = {Visual working memory; Bayes factors; Signal detection; Short-term memory},
	Month = {6},
	Pages = {210--219},
	Title = {Using Bayes factors to test the predictions of models: A case study in visual working memory},
	Ty = {JOUR},
