	Date-Added = {2020-10-16 13:45:41 +0100},
	Date-Modified = {2020-10-16 13:45:41 +0100},
	Keywords = {Obesity},
	Title = {Influence of being embodied in an obese virtual body on shopping behavior and products perception in VR},
	Ty = {JOUR},
