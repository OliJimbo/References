	Date-Added = {2020-10-16 13:45:41 +0100},
	Date-Modified = {2020-10-16 13:45:41 +0100},
	Keywords = {Stereotyping; Memory, Short-Term},
	Number = {4},
	Pages = {1604-1612},
	Title = {The effect of gender body-swap illusions on working memory and stereotype threat},
	Ty = {JOUR},
